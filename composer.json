{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "tymon/jwt-auth": "0.5.*",
        "d11wtq/boris": "~1.0",
        "elasticsearch/elasticsearch": "1.*",
        "aws/aws-sdk-php-laravel": "~3.0",
        "get-stream/stream-laravel": "~2.1",
        "sammyk/laravel-facebook-sdk": "^3.0",
        "RobBrazier/Piwik": "~2.0",
        "doctrine/dbal": "~2.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/fractal": "^0.12.0",
        "predis/predis": "~1.0",
        "shift31/laravel-elasticsearch": "^1.3",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/html": "^5.0",
        "zizaco/entrust": "dev-laravel-5",
        "laravel/socialite": "^2.0",
        "cocur/slugify": "^1.2",
        "google/apiclient": "1.0.*@beta",
        "sleeping-owl/admin": "2.x",
        "jenssegers/agent": "^2.2",
        "roumen/sitemap": "^2.5",
        "maatwebsite/excel": "~2.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "maxmind/web-service-common": "^0.0.4",
        "geoip2/geoip2": "^2.3",
        "davibennun/laravel-push-notification": "dev-laravel5",
        "behat/behat": "^3.0",
        "hocza/sendy": "1.*",
        "barryvdh/laravel-dompdf": "^0.6.1",
        "barryvdh/laravel-translation-manager": "^0.2.2",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "honeybadger-io/honeybadger-php": "~0.3",
        "facebook/graph-sdk": "^5.5",
        "facebook/facebook-instant-articles-sdk-php": "^1.6",
        "google/auth": "^1.3",
        "babychakra/babychakra-lumen": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers",
            "app/Transformers",
            "app/Console/Commands",
            "app/Models"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/babychakra/"},
        {"packagist.org": false}
    ]
}
